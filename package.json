  "dependencies": {
    "next": "^13.5.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "scripts": {
    "dev": "next dev",
    "build": "rm -rf build && next build && next export && mv ./out ./build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "deploy:prod": "yarn build && npx vercel ./build --confirm --no-clipboard --token=$VERCEL_TOKEN --prod",
    "deploy:preview": "node ./scripts/pullRequestPreview"
  },
  "devDependencies": {
    "eslint": "8.17.0",
    "eslint-config-next": "14.2.4",
    "eslint-config-prettier": "8.5.0",
    "prettier": "2.6.2",
    "eslint-plugin-prettier": "4.0.0"
  }
}
